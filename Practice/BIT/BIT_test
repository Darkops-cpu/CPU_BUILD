    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x559eed0bd470_0;
    %store/vec4 v0x559eed108b70_0, 0, 1;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x559eed0f69e0;
T_1 ;
    %vpi_call 2 12 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559eed0f69e0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559eed108c90_0, 0, 1;
