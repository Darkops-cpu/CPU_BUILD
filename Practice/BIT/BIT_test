    %store/vec4 v0x559eed108d60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559eed108c90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559eed108d60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559eed108c90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559eed108d60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559eed108c90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559eed108d60_0, 0, 1;
